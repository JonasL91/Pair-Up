<Application x:Class="PairUp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:properties="clr-namespace:PairUp.Properties"
             StartupUri="Views\MainScreen.xaml">
    <Application.Resources>
        <ResourceDictionary x:Key="ResourceDictionary">
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://Application:,,,/PairUpResources;component/ResourceDictionaries/ResourceLibrary.xaml"/>
                <ResourceDictionary Source="Themes/DataTemplates.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>

</Application>
